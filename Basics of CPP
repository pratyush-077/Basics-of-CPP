//EXPERIMENT 1B-BASIC CALCULATOR PROGRAM

#include <iostream>
using namespace std;

int main() {
    int num1, num2;
    cout << "enter the first number:";
    cin >> num1;
    cout << "enter the second number:";
    cin >> num2;

    int c;
    c = num1 + num2;
    cout << "sum=" << c << endl;

    int d;
    d = num1 - num2;
    cout << "difference=" << d << endl;

    int e;
    e = num1 * num2;
    cout << "multiply=" << e << endl;

    int f;
    f = num1 / num2;
    cout << "DIVISION=" << f << endl;

    return 0;
}
